#include <stdio.h>

// call by value
// it creates a copy of variable and manipulates it
// void increment(int a){
//     a = a+1;
//     printf("%d\n",a);
// }

// call by reference
// it does not make any copy 
// void increment(int *a){
//     // * -> dereferening operator
//     // & -> adddress of
//     *a = *a+1;
//     printf("%d\n",*a);
// }

int main() {
   // call by value and call by reference
   int a = 112;
//   &a -> address of a
  increment(&a);
  printf("%d",a);

    return 0;
}
